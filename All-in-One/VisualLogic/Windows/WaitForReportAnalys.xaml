<Window x:Class="All_in_One.VisualLogic.WaitForReportAnalys"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:All_in_One.VisualLogic"
        mc:Ignorable="d"
        Title="Bitte warten.." Height="200" Width="300">
    <Grid>
        <StackPanel>
            <Label Content="Die Logs werden ausgewertet, bitte warten..."></Label>
            <ProgressBar Name="ProgressLogAnalyse" Minimum="0" Maximum="100" Height="20" Margin="5" ></ProgressBar>
            <Label Name="ActualStep"></Label>
        </StackPanel>
    </Grid>
</Window>
