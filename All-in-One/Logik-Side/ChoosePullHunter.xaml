<Window x:Class="All_in_One.Logik_Side.ChoosePullHunter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:All_in_One.Logik_Side"
        mc:Ignorable="d"
        Title="ChoosePullHunter" Height="150" Width="300">
    <Grid>
        <StackPanel>
            <Label Content="Bitte Pull-Hunter für den Raid auswählen:" HorizontalContentAlignment="Center" Margin="10"></Label>
            <ComboBox Name="PullHunterList" ></ComboBox>
            <Button Name="Confirm" Content="Bestätige" Width="100" Height="30" Click="Confirm_Click" Margin="10" ></Button>
        </StackPanel>
    </Grid>
</Window>
